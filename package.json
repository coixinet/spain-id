{
  "name": "spain-id",
  "version": "1.0.10",
  "description": "Spanish id documents validator (NIF, CIF, NIE)",
  "main": "dist/index.umd.js",
  "module": "dist/index.modern.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent",
    "dev": "microbundle src/index.js",
    "patch": "npm version patch",
    "build": "microbundle src/index.js",
    "tdd": "jest --watchAll",
    "predeploy": "npm run test && npm run patch && npm run build",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coixinet/spain-id.git"
  },
  "keywords": [
    "spain",
    "id",
    "nif",
    "dni",
    "cif"
  ],
  "author": "Josep V. Monjo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coixinet/spain-id/issues"
  },
  "homepage": "https://github.com/coixinet/spain-id#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "acorn": ">=5.7.4",
    "core-js": "^2.6.10",
    "jest": "^25.2.7",
    "kind-of": ">=6.0.3",
    "microbundle": "^0.11.0",
    "minimist": ">=1.2.3",
    "serialize-javascript": ">=2.1.1"
  },
  "dependencies": {}
}
